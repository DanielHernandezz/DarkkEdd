<html style="background-color: gray">
    <body>
        <br><br><br>><br><br>
        <center>
        <table style="text-align: center" class="table is-bordered is-striped is-narrow is-hoverable container ">
            <tr>
                <th style="text-align: end" class="th is-link">NUEVO CLIENTE</th>
                <th class="th is-link"></th>
            </tr>
            <form action="clienteCreate" method="POST"> 
            <tr>
                <th style="text-align: center" class="th is-black">NOMBRE</th>
                <th class="th is-success"><input name="nombre" type="text" placeholder="Introducir Nombre" style="text-align: center" class="message is-dark"> </th>
            </tr>
            <tr>
                <th style="text-align: center" class="th is-black">DIRECCION</th>
                <th class="th is-success"><input name="direccion" type="text" placeholder="Introducir Direccion" style="text-align: center" class="message is-dark"> </th>
            </tr>
            <tr>
                <th style="text-align: center" class="th is-black">TELEFONO </th>
                <th class="th is-success"><input name="telefono" type="text" placeholder="Introducir Telefono" style="text-align: center" class="message is-dark"> </th>
            </tr>
            <tr>
                <th class="th is-link"></th>
                <th class="th is-link" style="text-align: center"><button class="button is-primary is-rounded">Agregar Cliente</button> </th>
            </tr>
            </form>
        </table>
            <br>
            <br>
            <div style="text-align: center">
              <a href="cliente" class="button is-dark is-rounded">Regresar</a>  
            </div>
            
        </div>
    </center>
    </body>
</html>